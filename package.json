      "typescript": "^5.1.6"
   },
   "dependencies": {
      "bcryptjs": "^2.4.3",
      "body-parser": "^1.20.2",
      "cors": "^2.8.5",
      "dotenv": "^16.3.1",
      "express": "^4.18.2",
      "generate-password-ts": "^1.6.3",
      "jsonwebtoken": "^9.0.1",
      "morgan": "^1.10.0",
      "multer": "^1.4.5-lts.1",
      "mysql2": "^3.5.2",
      "nodemailer": "^6.9.3",
      "nodemon": "^3.0.1",
      "pg": "^8.11.1",
      "reflect-metadata": "^0.1.13",
